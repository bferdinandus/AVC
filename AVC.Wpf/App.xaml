<Application
    x:Class="AVC.Wpf.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:viewModel="clr-namespace:AVC.Wpf.MVVM.ViewModel"
    xmlns:view="clr-namespace:AVC.Wpf.MVVM.View">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries />
            <DataTemplate DataType="{x:Type viewModel:VolumeSliderViewModel}">
                <view:VolumeSliderView />
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>